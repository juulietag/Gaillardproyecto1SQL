SELECT @@AUTOCOMMIT;
SET AUTOCOMMIT=0;

SELECT * FROM condicion_pago;

START TRANSACTION;
DELETE FROM condicion_pago
WHERE ID_plazo LIKE '%P000%';
SELECT * FROM condicion_pago;
ROLLBACK;
COMMIT;

SELECT * FROM usuario;

START TRANSACTION;
INSERT INTO usuario VALUES (30, 'REGISTRO GENÉRICO #1', 'REGISTRO GENÉRICO #1');
INSERT INTO usuario VALUES (40, 'REGISTRO GENÉRICO #2', 'REGISTRO GENÉRICO #2');
INSERT INTO usuario VALUES (50, 'REGISTRO GENÉRICO #3', 'REGISTRO GENÉRICO #3');
INSERT INTO usuario VALUES (60, 'REGISTRO GENÉRICO #4', 'REGISTRO GENÉRICO #4');
SAVEPOINT lote_1_4;
INSERT INTO usuario VALUES (70, 'REGISTRO GENÉRICO #5', 'REGISTRO GENÉRICO #5');
INSERT INTO usuario VALUES (80, 'REGISTRO GENÉRICO #6', 'REGISTRO GENÉRICO #6');
INSERT INTO usuario VALUES (90, 'REGISTRO GENÉRICO #7', 'REGISTRO GENÉRICO #7');
INSERT INTO usuario VALUES (100, 'REGISTRO GENÉRICO #8', 'REGISTRO GENÉRICO #8');
SAVEPOINT lote_5_8;
ROLLBACK TO lote_1_4;
COMMIT;
